<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Styles>
    <Style ss:ID="s64">
      <NumberFormat ss:Format="0.000"/>
    </Style>
  </Styles>
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
         <%model_class = Coordinate%>
        <Cell><Data ss:Type="String">Chainage</Data></Cell>
        <Cell><Data ss:Type="String"><%= model_class.human_attribute_name(:east_utm) %> </Data></Cell>
        <Cell><Data ss:Type="String"><%= model_class.human_attribute_name(:north_utm) %> </Data></Cell>
        <Cell><Data ss:Type="String"><%= model_class.human_attribute_name(:lattitude) %> </Data></Cell>
        <Cell><Data ss:Type="String"><%= model_class.human_attribute_name(:longitude) %> </Data></Cell>
        <Cell><Data ss:Type="String">Structure</Data></Cell>
        <!-- <Cell><Data ss:Type="String">Satellite View</Data></Cell> -->
      </Row>
      <% @coordinates.each_with_index do |coordinate, index| %>
      <Row>
        <Cell><Data ss:Type="String"><%= coordinate.title %></Data></Cell>
        <Cell ss:StyleID="s64"><Data ss:Type="Number"><%= coordinate.east_utm %></Data></Cell>
        <Cell ss:StyleID="s64"><Data ss:Type="Number"><%= coordinate.north_utm %></Data></Cell>
        <Cell><Data ss:Type="String"><%= coordinate.lattitude %></Data></Cell>
        <Cell><Data ss:Type="String"><%= coordinate.longitude %></Data></Cell>
        <Cell><Data ss:Type="String"><%= coordinate.description %></Data></Cell>
        <!-- <Cell><Data ss:Type="String"><%= "=HYPERLINK(CONCATENATE(\"https://www.google.com/maps/search/D#{index+2},E#{index+2}/data=!3m1!1e3\");\"Satellite View\")" %></Data></Cell> -->
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>